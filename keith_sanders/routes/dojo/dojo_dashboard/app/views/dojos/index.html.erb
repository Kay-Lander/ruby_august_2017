<h1>List of The Dojos around the nation</h1>
<table>
    <%  @dojo.each do |dojo| %>
    <tr>
        <th>Coding Dojo Branch</th>
        <th>street</th>
        <th>city</th>
        <th>state</th>
        <th>action</th>
    </tr>
    <tr>
        <td><%= dojo.branch %></td>
        <td><%= dojo.street %></td>
        <td><%= dojo.city %></td>
        <td><%= dojo.state %></td>
        <td><a href="/show/<%= dojo.id %>">Show</a> | <a href="/edit/<%= dojo.id %>">Edit</a> | <a href="/delete/<%= dojo.id %>" data-method='delete'>Delete</a></td>
    </tr>
     <% end %>
</table>

<a href="/new">New dojo</a>
